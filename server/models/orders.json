{
  "name": "orders",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      "client",
      "deliveryMember",
      "orderProducts"
    ]
  },
  "properties": {
    "code": {
      "type": "string"
    },
    "orderDate": {
      "type": "date",
      "required": true,
      "default": "$now"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "pending"
    },
    "totalPrice": {
      "type": "number"
    },
    "priceBeforeCoupon": {
      "type": "number"
    },
    "clientType": {
      "type": "string"
    },
    "note": {
      "type": "string"
    },
