{
  "name": "warehouseProducts",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "warehouse_product_index": {
      "keys": {
        "warehouseId": 1,
        "productAbstractId": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "scope": {
    "include": [
      "warehouse"
    ]
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "totalCount": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "expectedCount": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "available"
    },
    "threshold": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "accumulatedCountOverTime": {
      "type": "number",
      "required": true , 
      "default" : 0 
    },
    "avgBuyingPrice": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "warehouse": {
      "type": "belongsTo",
      "model": "warehouse",
      "foreignKey": "warehouseId"
    },
    "productAbstract": {
      "type": "belongsTo",
      "model": "productAbstract",
      "foreignKey": "productAbstractId"
    }
  },
  "acls": [],
  "methods": {}
}
