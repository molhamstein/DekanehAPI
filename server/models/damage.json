{
  "name": "damage",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      "warehouse",
      "damageProducts"
    ]
  },
  "properties": {
    "date": {
      "type": "date",
      "default": "$now"
    },
    "note": {
      "type": "string"
    },
    "reason": {
      "type": "string",
      "required": "true"
    }
  },
  "validations": [],
  "relations": {
    "warehouse": {
      "type": "belongsTo",
      "model": "warehouse",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "damageProducts": {
      "type": "hasMany",
      "model": "damageProduct",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
