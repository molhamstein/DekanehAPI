{
  "name": "userAward",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  
  "scope": {
    "include": [
      "awardPeriod"
    ]
  },
  "properties": {
    "progress": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "count": {
      "type": "number",
      "required": false,
      "default": 0
    },
    "complete": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "orders": {
      "type": "array",
      "required": false,
      "default": []
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user"
    },
    "award": {
      "type": "belongsTo",
      "model": "award"
    },
    "awardPeriod": {
      "type": "belongsTo",
      "model": "awardPeriod",
      "foreignKey": "awardPeriodId"
    }
  },
  "acls": [],
  "methods": {}
}