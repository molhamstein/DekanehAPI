{
  "name": "userNot",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      "user",
      "orders"
    ]
  },
  "properties": {
    "note": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "orders": {
      "type": "belongsTo",
      "model": "orders",
      "foreignKey": "orderId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
