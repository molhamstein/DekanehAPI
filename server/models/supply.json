{
  "name": "supply",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },  
  "scope": {
    "include": [
      "warehouse"
    ]
  },
  "properties": {
    "date": {
      "type": "date",
      "default": "$now"
    },
    "totalPrice": {
      "type": "number"
    },
    "note": {
      "type": "string"
    },
    "code": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "warehouse": {
      "type": "belongsTo",
      "model": "warehouse",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "supplyProducts": {
      "type": "hasMany",
      "model": "supplyProduct",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
