{
  "name": "orderProducts",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      "product"
    ]
  },
  "properties": {
    "sellingPrice": {
      "type": "number"
    },
    "count": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "creationDate": {
      "type": "date",
      "required": true,
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "product": {
      "type": "belongsTo",
      "model": "products"
    },
    "orders": {
      "type": "belongsTo",
      "model": "orders",
      "foreignKey": "orderId"
    },
    "productSnapshotRelation": {
      "type": "embedsOne",
      "model": "productSnapshot",
      "property": "productSnapshot",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
